{
    "name": "ReactNativeBase",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "postinstall": "sh fix_libs/fix_libs.sh && cd ios && pod install && cd .. && npx jetifier",
        "android_develop": "npx react-native run-android --variant=DevelopDebug --appId=com.kira.reactnativebase.develop",
        "android_staging": "npx react-native run-android --variant=StagingDebug --appId=com.kira.reactnativebase.staging",
        "android_product": "npx react-native run-android --variant=ProductDebug --appId=com.kira.reactnativebase.product",
        "ios_develop": "react-native run-ios --configuration=DevelopDebug --scheme=Develop --simulator=\"iPhone 14 Pro Max\"",
        "ios_staging": "react-native run-ios --configuration=StagingDebug --scheme=Staging",
        "ios_product": "react-native run-ios --configuration=ProductDebug --scheme=Product",
        "start": "export NODE_OPTIONS=--openssl-legacy-provider && npx react-native start",
        "test": "jest",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "postversion": "npx react-native-version --never-amend",
        "prepare": "husky install",
        "runAdv3": "~/Library/Android/sdk/emulator/emulator -avd Pixel_3a_API_29"
    },
